<ul class="list-group">
<li class="list-group-item">
  <%= simple_form_for @game, :html => { :class => 'form-horizontal' } do |f| %>
  <legend> Game Registration</legend>
    <div class="input-group">
    <%= f.input :name, class: 'form-control' %>
    </div>
    <div class="input-group">
    <%= f.input :address, :input_html =>{:id => 'gmaps-input-address'}, :placeholder => 'Start typing a place...' %>
    </div>
    <div class="input-group">
    <%= f.input :date_time, as: :datetime_picker, wrapper: :ranged_datetime %>
    </div>
    <div class="form-actions">
    <%= f.submit 'Create a game' %>
    </div>
  <% end %>
</li>
</ul>